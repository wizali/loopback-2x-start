{
    "name": "Student",
    "base": "User",
    "idInjection": true,
    "strict": true,
    "properties": {
        "name": {
            "type": "string",
            "required": true
        },
        "gender": {
            "type": "string",
            "required": false
        },
        "number": {
            "type": "string"
        }
    },
    "relations": {
        "class": {
            "type": "belongsTo",
            "model": "class",
            "foreignKey": "classId"
        }
    },
    "acls": [
        {
            "accessType": "*",
            "permission": "ALLOW",
            "principalType": "ROLE",
            "principalId": "$unauthenticated"
        }
    ]
}